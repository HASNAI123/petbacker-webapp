// app global css
a {
  color: #6c5493
}

.anchor-link {
  cursor:pointer
  color: $primary
}

.anchor-link:hover {
  text-shadow: 1px 1px 1px $secondary
}

.border-1 {
    border: 1px solid #e0e0e0;
}

.pac-container {
  z-index: 90000;
  position: fixed !important;
}

.max-width {
  max-width: 568px !important;
}
.border-none {
  border: none;
}
.background-select {
  -webkit-appearance: none;
  -moz-appearance: none;
  text-indent: 1px;
  text-overflow: '';
}
.margin-auto {
  margin-left: auto;
  margin-right: auto;
  display: block;
}
.force-select {
  -webkit-user-select: text;  /* Chrome 49+ */
  -moz-user-select: text;     /* Firefox 43+ */
  -ms-user-select: text;      /* No support yet */
  user-select: text;          /* Likely future */
}
.referral-title {
  font-size: 26px !important;
  font-weight: bold !important;
  margin-top: 25px !important;
  margin-bottom: 15px !important;
  line-height: 1.1 !important;
  text-rendering: optimizelegibility !important;
  color: #fff !important;
}
.background-blur {
  background:black;
  background:rgba(0,0,0,0.8);

  filter:blur(4px);
  -o-filter:blur(4px);
  -ms-filter:blur(4px);
  -moz-filter:blur(4px);
  -webkit-filter:blur(4px);
}

// icon colors
.star-color {
  color: $yellow-star;
}
.star-size{
  font-size: 12px;
}
.facebook-color {
  color: $facebook-blue;
}


// background color
.bg-neutral {
  background-color: $neutral
}
.bg-negative {
  background-color: $negative
}
.bg-positive {
  background-color: $positive
}
.bg-amber { 
  background-color: $$amber
}


// text type
.text-neutral{
  color: $neutral;
}
.text-positive{
  color: $positve;
}
.text-negative{
  color: $negative;
}
.text-primary{
  color: $primary !important;
}
.text-secondary{
  color: $secondary !important;
}
.text-label{
  color: $grey;
}

// font sizes
.font-x-small {
  font-size: 12px;
}
.font-small {
  font-size: 14px;
}
.font-normal {
  font-size: 16px;
}
.font-normal-bold {
  font-size: 16px;
  font-weight: 500;
}
.font-title {
  font-size: 18px;
}
.font-header-title {
  font-size: 18px !important;
  font-weight: 500 !important;
  color: black;
}
.font-header-title-large{
  font-size: 24px !important;
  font-weight: 500 !important;
  color: black;
}
.font-title-large {
  font-size: 24px;
}
.font-large {
  font-size: large;
}
.font-x-large {
  font-size: x-large;
}
.font-xx-large {
  font-size: xx-large;
}
.font-medium {
  font-size: 20px;
}
.font-medium-title {
  font-size: 20px !important;
  font-weight: 500 !important;
  color: black;
}


.border-lightgrey {
  border: 1px solid lightgrey;
}
.resize-none {
  resize: none;
}
.navbar {
  overflow: hidden;
  background-color: #333;
  text-transform: capitalize;
}
.navbar span {
  float: left;
  font-size: 14.4px;
  text-align: center;
  padding: 14px 16px;
  text-decoration: none;
  text-transform: uppercase;
  font-weight: 500;
  cursor: pointer
}
.dropdown {
  float: left;
}
.dropdown .dropbtn {
  font-size: 14.4px;
  border: none;
  outline: none;
  padding: 14px 16px;
  background-color: inherit;
  font-family: inherit;
  text-transform: uppercase;
  font-weight: 500;
  margin: 0;
}
.navbar .navbar-item:hover, .dropdown:hover .dropbtn {
  background-color: $neutral;
}
.dropdown-content {
  display: none;
  position: absolute;
  background: white;
  min-width: 160px;
  box-shadow: 0px 8px 16px 0px rgba(0,0,0,0.2);
  z-index: 1;
}
.dropdown-content span {
  float: none;
  color: black;
  padding: 12px 16px;
  text-decoration: none;
  display: block;
  text-align: left;
}
.dropdown-content span:hover {
  background-color: $neutral;
}
.dropdown:hover .dropdown-content {
  display: block;
}
.selected {
  border-bottom: 4px solid $primary
}
div.text-middle-line {
  width: 100%;
  text-align: center;
  border-bottom: 1px solid #c7c7c7;
  line-height: 0.1em;
  margin: 15px 0px;
  color: #8e8e8e;
}
.text-middle-line span {
  background:#fff;
  padding:0 10px;
}
.q-message-avatar{
  object-fit: cover;
}
img.q-message-avatar[src=""]{
  display: none;
}
.profile-img
  object-fit: cover

// cursor pointer
.clickable {
  cursor: pointer;
}


// notifications indicator
.notification-dot{
  height: 10px
  width: 10px
  background: $primary
  border-radius: 100px
  position: absolute
  right: 3px
  top: 7px
  background-color: $negative
}
.unread-notification{
  background-color: #DBEDF7
}

.no-select {
  -webkit-user-callout: none;
  -webkit-touch-callout: none; /* iOS Safari */
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-tap-highlight-color: transparent;
  -webkit-user-select: none; /* Safari */
  -khtml-user-select: none; /* Konqueror HTML */
  -moz-user-select: none; /* Old versions of Firefox */
  -ms-user-select: none; /* Internet Explorer/Edge */
  user-select: none; /* Non-prefixed version, currently supported by Chrome, Edge, Opera and Firefox */
}

.picked
  opacity: 0.45

// google maps styling
a[href^="http://maps.google.com/maps"]{
  display:none !important
}
a[href^="https://maps.google.com/maps"]{
  display:none !important
}
.gmnoprint a, .gmnoprint span, .gm-style-cc {
  xdisplay:none;
}
.gmnoprint div {
  background:none !important;
}

// image styling
.image-upload {
  object-fit: cover
}

.borderline {
  border: 1px solid lightgrey
}

// transitions
.fade-enter-active,
.fade-leave-active {
  transition-duration: 0.3s;
  transition-property: height, opacity;
  transition-timing-function: cubic-bezier(0,0.60,1,1);
  overflow: hidden;
}

.fade-enter,
.fade-leave-active {
  opacity: 0;
}

// dialog styling
.tip-bulb
  position: absolute
  top: 0
  left: calc(50% - 35px)
  padding: 10px
  background:  #fff
  width: 70px
  border-radius: 50%
  margin-top: -35px

.tip-bulb-image
  width: 50px

.tip-desc
  font-size: 16px
  color: #111

.tip-title
  font-size 20px
  color: #444

// dashboard height
.platform-ios .dashboard-ios
  height: 82vh;

.platform-android .dashboard-android
  height: 82.3vh; 

@media only screen and (min-height: 735px)
  .platform-android .dashboard-android
    height: 86vh;

@media only screen and (min-height: 1000px)
  .platform-android .dashboard-android
    height: 90vh;

// listing styling
.backer-listing
  width: 150px !important

.listing-image
  object-fit: cover
  min-height: 145px
  max-height: 145px
  width: 140px  

.broadcast-image
  width: 358px

@media only screen and (min-width: 320px)
  .backer-listing
    width: 140px !important
  .listing-image
    width: 140px
  .broadcast-image
    width: 285px;
  .image-offset
    width: 120%;

@media only screen and (min-width: 350px)
  .backer-listing
    width: 160px !important
  .listing-image
    width: 156px
  .broadcast-image
    width: 330px;
  .image-offset
    width: 330px;

@media only screen and (min-width: 370px)
  .backer-listing
    width: 170px !important
  .listing-image
    width: 163px
  .broadcast-image
    width: 340px;
  .image-offset
    width: 340px;

@media only screen and (min-width: 410px)
  .backer-listing
    width: 190px !important
  .listing-image
    width: 183px
  .broadcast-image
    width: 380px;
  .image-offset
    width: 380px;

@media only screen and (min-width: 425px)
  .backer-listing
    width: 195px !important
  .listing-image
    width: 188px
  .broadcast-image
    width: 390px;
  .image-offset
    width: 390px;

@media only screen and (min-width: 500px)
  .backer-listing
    width: 200px !important
  .listing-image
    width: 200px
  .image-offset
    width: 400px;

@media only screen and (min-width: 700px)
  .backer-listing
    width: 200px !important
  .listing-image
    width: 200px
  .broadcast-image
    width: 700px
  .image-offset
    width: 700px

// moment styling
.liked
  color: $primary

// progress bar styling
.q-linear-progress .q-linear-progress__track, .q-linear-progress__model {
  border-radius: 20px
}

// input styling
.input {
  box-shadow: 1px 2px 5px lightgrey
  border-radius: 7px
  padding-left: 10px
  padding-right: 10px
}
.text-wrap {
  white-space: nowrap
  overflow: hidden
  text-overflow: ellipsis
}
.pre-formatted {
  white-space: pre;
}
.selected-button {
  background-color: $primary
  color: white
  font-color: white
}

.q-dialog__inner--maximized > .q-card {
  padding-top: env(safe-area-inset-top);
}
.q-dialog__inner--maximized > .q-card {
  .q-toolbar.fixed-top {
    padding-top: env(safe-area-inset-top);
  }
}

// cordova specific styling
.q-infinite-scroll__loading
  width: 100%

body.q-ios-padding {
  padding-bottom: 0px !important;
}

body.q-ios-padding .q-dialog__inner{
  padding-bottom: 0px !important;
}

body.q-ios-padding .q-dialog__inner--maximized {
  padding-bottom: 0px !important;
  padding-top: 0px !important;
  .q-dialog .q-card {
    box-shadow: none;
  }
}

body.q-ios-padding .q-dialog__inner > div {
  max-height: 100% !important;
}

body.q-body--prevent-scroll {
  #q-app .q-page-container {
    height: 100vh;
  }

  -webkit-user-select: none;
  -webkit-user-callout: none;
  -webkit-touch-callout: none; /* iOS Safari */
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-tap-highlight-color: transparent;
  -webkit-user-select: none; /* Safari */
  -khtml-user-select: none; /* Konqueror HTML */
  -moz-user-select: none; /* Old versions of Firefox */
  -ms-user-select: none; /* Internet Explorer/Edge */
  user-select: none; /* Non-prefixed version, currently supported by Chrome, Edge, Opera and Firefox */
}
.vue-lb-open {
  overflow: visible !important;
}

div.text-middle-line
  width: 100%
  text-align: center
  border-bottom: 1px solid #c7c7c7
  line-height: 0.1em
  margin: 15px 0px
  color: #8e8e8e

.text-middle-line span
  background:#fff
  padding:0 10px

// hide scrollbar
.hide-scrollbar {
  -ms-overflow-style: none;  /* IE and Edge */
  scrollbar-width: none;  /* Firefox */
}
.hide-scrollbar::-webkit-scrollbar {
  display: none;
}

// opaque body
.opaque-body{
  background: white !important;
  opacity: 1 !important;
}

// driver js 
.driver-fix-stacking
  z-index: 100003 !important;
  opacity: 1.0 !important;
  -webkit-transform: none !important;
  -moz-transform: none !important;
  -ms-transform: none !important;
  -o-transform: none !important;
  transform: none !important;
  -webkit-filter: none !important;
  -moz-filter: none !important;
  -ms-filter: none !important;
  -o-filter: none !important;
  filter: none !important;
  -webkit-perspective: none !important;
  -moz-perspective: none !important;
  -ms-perspective: none !important;
  -o-perspective: none !important;
  perspective: none !important;
  -webkit-transform-style: flat !important;
  -moz-transform-style: flat !important;
  -ms-transform-style: flat !important;
  transform-style: flat !important;
  -webkit-transform-box: border-box !important;
  -moz-transform-box: border-box !important;
  -ms-transform-box: border-box !important;
  -o-transform-box: border-box !important;
  transform-box: border-box !important;
  will-change: unset !important;

.service_time input {
  padding-top: 2px !important
}

.white-link a {
  color: white !important
}